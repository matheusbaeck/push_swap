CFLAGS = -Wall -Wextra -Werror -g3 -fsanitize=address

ARG8="3 4 6 1 2 5 7 0"
ARG100="86 288 455 346 179 60 318 412 336 47 447 456 186 452 72 411 274 15 284 370 390 335 273 121 55 52 25 413 484 152 218 114 450 231 317 9 45 328 266 169 372 226 420 123 181 113 145 438 241 333 316 105 102 6 460 240 295 402 271 311 306 496 407 198 112 489 250 230 196 14 267 297 191 54 58 337 89 183 201 252 228 220 444 248 430 439 304 216 278 39 251 134 303 419 100 2 242 292 432 422"
ARG500="375 300 144 70 426 391 323 381 339 409 83 87 183 27 69 123 462 463 99 101 386 34 17 328 43 65 109 406 472 310 11 343 410 213 261 241 274 280 238 136 385 237 260 404 251 249 229 81 383 297 496 272 102 239 211 164 384 347 301 105 45 368 100 335 298 333 245 482 162 317 484 171 362 486 459 457 464 242 153 86 309 131 149 60 117 332 267 152 186 203 460 160 47 218 387 475 315 281 172 246 446 414 68 124 168 382 38 286 422 142 461 20 305 440 6 53 174 176 395 158 290 79 429 18 89 495 331 443 146 7 187 197 161 8 121 282 458 296 16 179 292 177 389 231 412 318 452 264 425 77 95 283 401 399 202 74 206 402 465 337 312 311 466 487 254 417 320 265 58 22 374 195 182 227 96 392 215 293 485 118 247 437 420 219 156 5 345 76 336 2 230 223 481 112 234 431 497 291 9 36 59 330 278 39 61 175 306 456 4 220 40 307 400 277 115 248 444 170 145 359 46 130 344 72 491 418 449 44 365 424 273 353 80 326 113 252 82 23 98 363 313 151 64 129 304 288 361 21 447 407 348 119 327 42 50 66 451 226 388 208 14 52 106 271 224 369 173 244 243 411 397 255 378 15 358 207 253 258 85 193 394 210 471 329 372 259 51 467 133 88 430 423 126 196 442 377 31 439 235 10 354 78 325 268 448 316 154 275 285 473 48 122 263 29 184 342 240 167 159 270 166 357 453 287 19 432 494 476 408 111 221 26 148 94 435 470 438 360 225 135 140 398 469 150 405 479 107 441 433 63 104 55 488 415 340 54 199 56 134 141 137 178 434 269 138 217 216 314 321 455 128 62 155 338 355 346 33 308 12 90 390 349 24 302 73 132 198 192 71 143 403 370 367 276 356 214 93 108 364 67 185 212 350 371 190 322 91 103 492 419 222 319 256 490 147 30 188 474 257 478 1 393 92 499 250 427 165 233 236 228 28 351 194 376 294 189 413 480 37 352 84 57 116 127 75 289 379 3 299 396 380 493 180 125 500 295 110 341 454 163 468 334 284 428 114 498 232 421 35 445 373 139 303 489 25 49 266 32 324 483 157 279 209 450 13 477 366 200 120 191 97 201 262 41 181 416 204 169 436 205"

all: $(NAME)

$(NAME): $(OBJS)
	$(CC) $(CFLAGS) $(OBJS) libft.a -o $(NAME)

clean: 
	rm -f $(OBJS)

fclean: clean
	rm -f $(NAME)

re: clean all

8:
	@./$(NAME) $(ARG8) | wc -l
	@./$(NAME) $(ARG8) | ./checker_Mac $(ARG8)

100:
	@./$(NAME) $(ARG100) | wc -l
	@./$(NAME) $(ARG100) | ./checker_Mac $(ARG100)


500:
	@./$(NAME) $(ARG500) | wc -l
	@./$(NAME) $(ARG500) | ./checker_Mac $(ARG500)
	