all: $(NAME) clean

$(NAME): $(OBJS) clean
	$(CC) $(CFLAGS) $(OBJS) libft.a -o $(NAME)

libft.a:
	cp -f ../../#lib/libft.a ./libft.a
	cp -f ../../#header/libft.h ./libft.h

clean: 
	rm -f $(OBJS)

fclean: clean
	rm -f $(NAME)

re: clean all

100: all
	@./$(NAME) $(ARG100)
	@./$(NAME) $(ARG100) | wc -l
	@./$(NAME) $(ARG100) | ./checker_Mac $(ARG100)

500: all
